{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended"
    // "prettier",
    // "prettier/react"
  ],
  "plugins": [
    "jsx-a11y"
    // "prettier"
  ],
  "rules": {
    "semi": 1,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prop-types": [
      0
    ],
    "no-underscore-dangle": [
      0
    ],
    "react/jsx-props-no-spreading": [
      0
    ]
    // "prettier/prettier": [
    //   "error",
    //   {
    //     "semi": true
    //   }
    // ]
  }
}